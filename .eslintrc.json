{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "linebreak-style": "off",
        "eol-last": "off",
        "no-console": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-unused-vars": "off",
        "class-methods-use-this": "off",
        "no-useless-constructor": "off",
        "no-empty-function": "off",
        "camelcase": "off"
    }
}
